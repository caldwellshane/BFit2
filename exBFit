OUTFILE="BFit2_"$BFIT".out"
ROOTFILE="BFit2_"$BFIT".root"
#####################
if (($SAVE)); then
	rm $OUTFILE #BFit2_$BFIT.out
	./BFit2 $BDN $BFIT > $OUTFILE
else
	./BFit2 $BDN $BFIT
fi
cp -a BFit.root $ROOTFILE #BFit2_$BFIT.root
#####################
echo -en "\007"

